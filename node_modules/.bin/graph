#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/saeeddawod/Desktop/Studio/work/scs/solidity-statemachine/node_modules/.pnpm/@graphprotocol+graph-cli@0.68.5_@types+node@20.12.7_node-fetch@3.3.2_typescript@5.4.5/node_modules/@graphprotocol/graph-cli/bin/node_modules:/Users/saeeddawod/Desktop/Studio/work/scs/solidity-statemachine/node_modules/.pnpm/@graphprotocol+graph-cli@0.68.5_@types+node@20.12.7_node-fetch@3.3.2_typescript@5.4.5/node_modules/@graphprotocol/graph-cli/node_modules:/Users/saeeddawod/Desktop/Studio/work/scs/solidity-statemachine/node_modules/.pnpm/@graphprotocol+graph-cli@0.68.5_@types+node@20.12.7_node-fetch@3.3.2_typescript@5.4.5/node_modules/@graphprotocol/node_modules:/Users/saeeddawod/Desktop/Studio/work/scs/solidity-statemachine/node_modules/.pnpm/@graphprotocol+graph-cli@0.68.5_@types+node@20.12.7_node-fetch@3.3.2_typescript@5.4.5/node_modules:/Users/saeeddawod/Desktop/Studio/work/scs/solidity-statemachine/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/saeeddawod/Desktop/Studio/work/scs/solidity-statemachine/node_modules/.pnpm/@graphprotocol+graph-cli@0.68.5_@types+node@20.12.7_node-fetch@3.3.2_typescript@5.4.5/node_modules/@graphprotocol/graph-cli/bin/node_modules:/Users/saeeddawod/Desktop/Studio/work/scs/solidity-statemachine/node_modules/.pnpm/@graphprotocol+graph-cli@0.68.5_@types+node@20.12.7_node-fetch@3.3.2_typescript@5.4.5/node_modules/@graphprotocol/graph-cli/node_modules:/Users/saeeddawod/Desktop/Studio/work/scs/solidity-statemachine/node_modules/.pnpm/@graphprotocol+graph-cli@0.68.5_@types+node@20.12.7_node-fetch@3.3.2_typescript@5.4.5/node_modules/@graphprotocol/node_modules:/Users/saeeddawod/Desktop/Studio/work/scs/solidity-statemachine/node_modules/.pnpm/@graphprotocol+graph-cli@0.68.5_@types+node@20.12.7_node-fetch@3.3.2_typescript@5.4.5/node_modules:/Users/saeeddawod/Desktop/Studio/work/scs/solidity-statemachine/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphprotocol/graph-cli/bin/run" "$@"
else
  exec node  "$basedir/../@graphprotocol/graph-cli/bin/run" "$@"
fi
